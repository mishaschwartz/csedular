<%= form_with model: [@resource, @availability], local: true, class: 'create-update-form' do |f| %>
  <div class='form-input'>
    <%= f.label :start_time %>
    <%= f.datetime_local_field :start_time %>
  </div>
  <div class='form-input'>
    <%= f.label :end_time %>
    <%= f.datetime_local_field :end_time %>
  </div>
  <div>
    <%= submit_tag %>
  </div>
<% end %>